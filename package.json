{
  "name": "@powerhousedao/vetra-builder-package",
  "description": "",
  "version": "0.0.34",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "/dist"
  ],
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "staging"
      },
      {
        "name": "dev",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "exports": {
    ".": {
      "development": "./index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "development": "./document-models/index.ts",
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "development": "./editors/index.ts",
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./subgraphs": {
      "development": "./subgraphs/index.ts",
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "development": "./processors/index.ts",
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "development": "./powerhouse.manifest.json",
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "build:css": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "build:css:watch": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css --watch",
    "build": "npm run build:tsc && npm run build:css",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "latest",
    "@powerhousedao/common": "latest",
    "@powerhousedao/connect": "^5.0.1",
    "@powerhousedao/design-system": "latest",
    "@powerhousedao/document-engineering": "^1.38.0",
    "@renown/sdk": "latest",
    "document-model": "latest",
    "graphql": "^16.10.0",
    "graphql-request": "^7.2.0",
    "graphql-tag": "^2.12.6",
    "kysely": "^0.28.7",
    "luxon": "^3.7.2",
    "nanoid": "^5.1.6",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@eslint/js": "^9.37.0",
    "@openfeature/core": "^1.9.1",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "latest",
    "@powerhousedao/ph-cli": "latest",
    "@powerhousedao/reactor-api": "latest",
    "@powerhousedao/reactor-browser": "latest",
    "@powerhousedao/reactor-local": "latest",
    "@powerhousedao/scalars": "latest",
    "@powerhousedao/switchboard": "latest",
    "@powerhousedao/vetra": "latest",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/cli": "^4.1.14",
    "@types/node": "^22.13.11",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "adm-zip": "^0.5.16",
    "document-drive": "latest",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}