{
  "name": "@powerhousedao/vetra-builder-package",
  "description": "",
  "version": "0.0.3",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "/dist"
  ],
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "staging"
      },
      {
        "name": "dev",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./document-models": {
      "types": "./dist/document-models/index.d.ts",
      "import": "./dist/document-models/index.js"
    },
    "./editors": {
      "types": "./dist/editors/index.d.ts",
      "import": "./dist/editors/index.js"
    },
    "./document-models/*": {
      "types": "./dist/document-models/*/index.d.ts",
      "import": "./dist/document-models/*/index.js"
    },
    "./editors/*": {
      "types": "./dist/editors/*/index.d.ts",
      "import": "./dist/editors/*/index.js"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs/index.d.ts",
      "import": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts",
      "import": "./dist/processors/index.js"
    },
    "./manifest": "./dist/powerhouse.manifest.json",
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "lint": "eslint --config eslint.config.js --cache --cache-strategy content",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "build:css": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "build:css:watch": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css --watch",
    "build": "npm run tsc && npm run tailwind",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tailwind": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "check-circular-imports": "npx dpdm -T ./index.ts",
    "vetra": "ph-cli vetra",
    "migrate": "ph-cli migrate"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "staging",
    "@powerhousedao/common": "staging",
    "@powerhousedao/design-system": "staging",
    "@powerhousedao/document-engineering": "^1.38.0",
    "document-model": "staging",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "kysely": "^0.28.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.12",
    "@eslint/js": "^9.38.0",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "staging",
    "@powerhousedao/config": "staging",
    "@powerhousedao/ph-cli": "staging",
    "@powerhousedao/reactor-api": "staging",
    "@powerhousedao/reactor-browser": "staging",
    "@powerhousedao/reactor-local": "staging",
    "@powerhousedao/scalars": "staging",
    "@powerhousedao/switchboard": "staging",
    "@powerhousedao/vetra": "staging",
    "@powerhousedao/connect": "staging",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/cli": "^4.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "document-drive": "staging",
    "eslint": "^9.38.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "package-manager-detector": "^0.2.8",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "semantic-release": "^24.2.8",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}